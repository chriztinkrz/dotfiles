{
  "detectedCompositor": "niri",
  "cheatsheetData": [
    {
      "title": "Applications",
      "binds": [
        {
          "keys": "SUPER+F5",
          "desc": "spawn-sh \"qs -c noctalia-shell ipc call plugin:screen-recorder toggle\""
        },
        {
          "keys": "SUPER+N",
          "desc": "spawn-sh \"qs -c noctalia-shell ipc call notifications dismissAll\""
        },
        {
          "keys": "SUPER+GRAVE",
          "desc": "spawn-sh \"qs -c noctalia-shell ipc call wallpaper toggle\""
        },
        {
          "keys": "SUPER+I",
          "desc": "noctalia-settings"
        },
        {
          "keys": "SUPER+ALT+V",
          "desc": "noctalia-clipboard"
        },
        {
          "keys": "SHIFT+ALT+V",
          "desc": "noctalia-emoji_picker"
        },
        {
          "keys": "SUPER+Escape",
          "desc": "noctalia-power-menu"
        },
        {
          "keys": "SUPER+L",
          "desc": "Lock Screen: noctalia-lock"
        },
        {
          "keys": "SHIFT+F2",
          "desc": "increase_brightness"
        },
        {
          "keys": "SHIFT+F1",
          "desc": "decrease_brightness"
        },
        {
          "keys": "SUPER+RETURN",
          "desc": "Open Terminal: foot"
        },
        {
          "keys": "SUPER+SPACE",
          "desc": "Open App Launcher: noctalia-app_launcher"
        },
        {
          "keys": "SUPER+B",
          "desc": "Open Browser: zen"
        },
        {
          "keys": "SUPER+V",
          "desc": "noctalia-clipboard"
        },
        {
          "keys": "SUPER+E",
          "desc": "File Manager: nautilus"
        },
        {
          "keys": "ALT+F2",
          "desc": "spawn-sh \"wpctl set-volume @DEFAULT_AUDIO_SINK@ 0.05+\""
        },
        {
          "keys": "ALT+F1",
          "desc": "spawn-sh \"wpctl set-volume @DEFAULT_AUDIO_SINK@ 0.05-\""
        },
        {
          "keys": "ALT+F3",
          "desc": "spawn-sh \"wpctl set-mute @DEFAULT_AUDIO_SINK@ toggle\""
        },
        {
          "keys": "Mic Mute",
          "desc": "spawn-sh \"wpctl set-mute @DEFAULT_AUDIO_SOURCE@ toggle\""
        },
        {
          "keys": "F1",
          "desc": "spawn-sh \"playerctl previous\""
        },
        {
          "keys": "F2",
          "desc": "spawn-sh \"playerctl play-pause\""
        },
        {
          "keys": "F3",
          "desc": "spawn-sh \"playerctl next\""
        }
      ]
    },
